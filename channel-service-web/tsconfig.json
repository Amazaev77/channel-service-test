{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./src/",
    "paths": {
      "@components/*": ["/components/*"],
      "@assets/*": ["/assets/*"],
      "@hooks/*": ["/hooks/*"],
      "@services/*": ["/services/*"],
      "@models/*": ["/models/*"],
      "@store/*": ["/store/*"],
      "@pages/*": ["/pages/*"],
      "@config/*": ["/config/*"],
      "@hoc/*": ["/hoc/*"]
    },
  },
  "include": ["react-app-env.d.ts", "**/*.ts", "**/*.tsx", "**/*.svg", "**/*.png"]
}
